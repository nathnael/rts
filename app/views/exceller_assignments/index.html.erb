<p id="notice"><%= notice %></p>

<h1>Exceller Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Exceller</th>
      <th>Project</th>
      <th>Skill</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Status</th>
      <th>Created by</th>
      <th>Modified by</th>
      <th>Deleted at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exceller_assignments.each do |exceller_assignment| %>
      <tr>
        <td><%= exceller_assignment.exceller_id %></td>
        <td><%= exceller_assignment.project_id %></td>
        <td><%= exceller_assignment.skill_id %></td>
        <td><%= exceller_assignment.start_date %></td>
        <td><%= exceller_assignment.end_date %></td>
        <td><%= exceller_assignment.status %></td>
        <td><%= exceller_assignment.created_by %></td>
        <td><%= exceller_assignment.modified_by %></td>
        <td><%= exceller_assignment.deleted_at %></td>
        <td><%= link_to 'Show', exceller_assignment %></td>
        <td><%= link_to 'Edit', edit_exceller_assignment_path(exceller_assignment) %></td>
        <td><%= link_to 'Destroy', exceller_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exceller Assignment', new_exceller_assignment_path %>
